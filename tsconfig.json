{
  "compilerOptions": {
    "alwaysStrict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "./dist",
    "skipLibCheck": true,
    "strict": true,
    "strictFunctionTypes": true,
    "target": "ES2020",
    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ]
  }
}